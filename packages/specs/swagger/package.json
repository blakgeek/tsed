{
  "name": "@tsed/swagger",
  "description": "Swagger package for Ts.ED framework",
  "type": "module",
  "version": "7.69.4",
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels &&  yarn build:ts && yarn copy:views && yarn copy:scripts",
    "copy:views": "cp -r ./views ./lib/views",
    "copy:scripts": "cp -r ./scripts/constants.js ./lib/esm/constants.js",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "start": "ts-node test/app/index.ts",
    "test": "jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "@tsed/normalize-path": "workspace:*",
    "@tsed/openspec": "workspace:*",
    "fs-extra": "11.2.0",
    "micromatch": "4.0.5",
    "swagger-ui-dist": ">=5.17.1",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/common": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/json-mapper": "workspace:*",
    "@tsed/openspec": "workspace:*",
    "@tsed/schema": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/swagger-ui-dist": "3.30.4",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@tsed/common": "7.69.4"
  },
  "peerDependenciesMeta": {
    "@tsed/common": {
      "optional": false
    }
  }
}
