{
  "name": "@tsed/json-mapper",
  "description": "Json mapper module for Ts.ED Framework",
  "type": "module",
  "version": "7.69.4",
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/json-mapper.umd.cjs",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "browser": "./lib/browser/json-mapper.umd.cjs",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels && yarn build:ts && yarn run build:browser",
    "build:browser": "vite build",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json mapper",
    "mapper",
    "serialization",
    "deserialization",
    "class",
    "classes",
    "tsed"
  ],
  "dependencies": {
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/core": "workspace:*",
    "@tsed/schema": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "typescript": "5.4.5",
    "vite": "5.2.11"
  },
  "peerDependencies": {
    "@tsed/core": "7.69.4",
    "@tsed/schema": "7.69.4"
  },
  "peerDependenciesMeta": {
    "@tsed/core": {
      "optional": false
    },
    "@tsed/schema": {
      "optional": false
    }
  }
}
