{
  "name": "@tsed/core",
  "description": "Core module for Ts.ED Framework",
  "type": "module",
  "version": "7.69.4",
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/core.umd.cjs",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "browser": "./lib/browser/core.umd.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts && yarn run build:browser",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\"",
    "build:browser": "vite build",
    "test": "jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/typescript": "workspace:*",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "typescript": "5.4.5",
    "vite": "5.2.11"
  }
}
