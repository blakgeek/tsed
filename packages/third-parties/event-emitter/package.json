{
  "name": "@tsed/event-emitter",
  "description": "EventEmitter package for Ts.ED framework",
  "type": "module",
  "version": "7.69.4",
  "contributors": [
    {
      "name": "Oliver Christen"
    }
  ],
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/event-emitter.umd.cjs",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "browser": "./lib/browser/event-emitter.umd.cjs",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts && yarn run build:browser",
    "build:browser": "vite build",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/common": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "eventemitter2": "^6.4.9",
    "jest": "^29.7.0",
    "typescript": "5.4.5",
    "vite": "5.2.11"
  },
  "peerDependencies": {
    "eventemitter2": "^6.4.4"
  }
}
